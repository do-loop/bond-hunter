<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>vk.ignis</Authors>
    <Product>BondHunter</Product>
    <PackageId>BondHunter</PackageId>
    <PackageTags>ip, location, sdk</PackageTags>
